{
  "name": "Comet",
  "functions": [ {
    "module": "./workflows/comet/comet.js"
  } ], {
    "name": "genTimeWindows",
    "module": "./workflows/comet/comet.js"
  }, {
    "name": "genDataSets",
    "module": "./workflows/comet/comet.js"
  }, {
    "name": "plotData",
    "module": "./workflows/comet/comet.js"
  } ],
  "processes": [ {
    "name": "GenData",
    "function": "genXmlCollection",
    "ins": [ "xmlData", "config" ],
    "outs": [ "stations:dataSetsCount"]
  }, {
    "name": "GenTimeWindows",
    "function": "genTimeWindows",
    "ins": [ "xmlData", "config" ],
    "outs": [ "stations:dataSetsCount"]
  }, {
    "name": "GenDataSets",
    "function": "genDataSets",
    "ins": [ "stations" ],
    "outs": [ "dataSet" ]
  }, {
    "name": "PlotGraphs",
    "function": "plotData",
    "ins": ["dataSet"],
    "outs": []
  } ],
  "signals": [ {
    "name": "xmlData",
    "data": [ { "path": "./data.xml" } ]
  }, {
    "name": "counties"
  }, {
    "name": "stations"
  }, {
    "name": "config",
    "data": [ { "xpath": "//Collection[@label='station']" } ]
  }, {
    "name": "dataSet"
  }, {
    "name": "dataSetsCount",
    "control": "count"
  } ],
  "ins": [],
  "outs": []
}
