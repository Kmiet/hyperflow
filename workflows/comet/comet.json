{
  "name": "Comet",
  "functions": [ {
    "name": "genXmlCollection",
    "module": "./workflows/comet/comet.js"
  }, {
    "name": "genTimeWindows",
    "module": "./workflows/comet/comet.js"
  }, {
    "name": "genDataSets",
    "module": "./workflows/comet/comet.js"
  }, {
    "name": "plotData",
    "module": "./workflows/comet/comet.js"
  } ],
  "processes": [ {
    "name": "GenData",
    "function": "genXmlCollection",
    "ins": [ "xmlData", "config" ],
    "outs": [ "stations:dataSetsCount"]
  }, {
    "name": "GenDataSets",
    "function": "genDataSets",
    "ins": [ "stations" ],
    "outs": [ "dataSet" ]
  }, {
    "name": "PlotGraphs",
    "function": "plotData",
    "ins": ["dataSet"],
    "outs": []
  } ],
  "signals": [ {
    "name": "xmlData",
    "data": [ { "path": "./workflows/comet/data.xml" } ]
  }, {
    "name": "counties"
  }, {
    "name": "stations"
  }, {
    "name": "timeWindow"
  }, {
    "name": "config",
    "data": [ { 
       "xpath": "//Collection[@label='station']",
       "start_time": "1.196499599E9",
       "end_time": "1.197359999E9"
    } ]
  }, {
    "name": "dataSet"
  }, {
    "name": "dataSetsCount",
    "control": "count"
  } ],
  "ins": [],
  "outs": []
}
